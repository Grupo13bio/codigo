from Bio import SeqIO

record = SeqIO.read("sequence.gb", "genbank")

print record

featcds = []
seqDNA=[]
annotations=[],[]

for f in record.features:
   print f.type + " " + str(f.location)


for i in xrange(len(record.features)):
    if record.features[i].type == "CDS":
        featcds.append(i)

print "Features CDS"
print len(featcds)

for k in featcds:
    seqDNA.append(record.features[k].qualifiers['translation'][0])
    annotations[0].append(record.features[k].qualifiers['protein_id']) 

    if 'function' in record.features[k].qualifiers:
       annotations[1].append(record.features[k].qualifiers['function'])
    else:
       annotations[1].append('-')

for k in featcds:
   
    seqDNA =  record.features[k].extract(record.seq)
    print seqDNA.translate()
